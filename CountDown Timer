//Write countdown(n) which logs n, n-1, â€¦, 0 to the console at 1-second intervals, then logs "done" and stops.

function countdown(n){
    let timer = setInterval(()=>{
        console.log(n);
        n--;
        if(n<0){
            console.log("done");
            clearInterval(timer)
        }
    }, 1000)
}

countdown(5)
